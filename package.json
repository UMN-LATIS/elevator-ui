    "@tanstack/vue-query": "^5.83.0",
    "@tanstack/vue-table": "^8.21.3",
    "@turf/circle": "^6.5.0",
    "@umn-cla/uppy-aws-s3": "^0.1.1",
    "@uppy/core": "^4.4.7",
    "@uppy/dashboard": "^4.3.4",
